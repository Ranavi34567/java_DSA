class Main {
    public static void main(String[] args) {
        //System.out.println("Try programiz.pro");
        int[] arr={5,9,1,8,7};
        int sum=0;
        int n=arr.length;
        for(int i=0;i<=n-3;i++){
            for(int j=i;j<i+3;j++){
                System.out.print(arr[j]+" ");
            }
            System.out.println();
        }
    }
}
