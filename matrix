class Main {
    public static void main(String[] args) {
        //System.out.println("Try programiz.pro");
        int[][] arr={{1,5,6,3},{3,1,9,4},{2,6,1,5}};
        int r=arr.length;
        int c=arr[0].length;
        int i=0;
        int j=1;
        boolean boom=true;
        int temp=arr[i][j];
        while(i<r && j<c){
            if(arr[i][j]!=temp){
               boom=false;
            }
            i=i+1;
            j=j+1;
        }
        System.out.println(boom);
    }
}
