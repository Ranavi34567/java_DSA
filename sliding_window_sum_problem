class Main {
    public static void main(String[] args) {
        //System.out.println("Try programiz.pro");
        int[] arr={1,2,3,1,4};
        int n=arr.length;
        int c=0;
        int sl=3;
        int thre=7;
        for(int r=0;r<=n-sl;r++ ){
            int sum=0;
            for(int j=r;j<r+sl;j++){
                sum+=arr[j];
            }
            if(sum>=thre){
                c++;
            }
        }
        System.out.println(c);
    }
}
