class Main {
    public static void main(String[] args) {
        //System.out.println("Try programiz.pro");
        int[] arr={1,4,6,7};
        int n=arr.length;
        for(int i=0;i<n;i++){
            for(int j=i;j<n;j++){
                for(int k=i;k<=j;k++){
                    System.out.print(arr[k] + " ");
                }
                System.out.println();
            }
        }
    }
}
