class Main {
    public static void main(String[] args) {
        //System.out.println("Try programiz.pro");
        String arr="ammu";
        int n=arr.length();
        for(int i=0;i<n;i++){
            for(int j=i;j<=n;j++){
                System.out.println(arr.substring(i,j));
            }
            //System.out.println();
        }
    }
}
